{
  "name": "rover",
  "version": "0.0.0",
  "description": "Rover programming test.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "swc src --out-dir dist",
    "build-watch": "swc src -w --out-dir dist",
    "start": "NODE_ENV=production node dist/index.js",
    "start-watch": "nodemon --watch \"dist/**/**\" -e js ./dist/index.js",
    "clean": "rm -rf dist",
    "dev": "concurrently \"npm:build-watch\" \"npm:start-watch\""
  },
  "author": "Yves BodÃª",
  "license": "MIT",
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.229",
    "@types/node": "^18.7.2",
    "chokidar": "^3.5.3",
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19",
    "typescript": "^4.7.4"
  }
}
